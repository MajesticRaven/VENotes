{
	font-size: 19px;
	font-family: Tahoma;
}
	QLineEdit#lineEdit_reg_log:hover, #lineEdit_reg_pass:hover, #lineEdit_reg_pass_check:hover, #lineEdit_reg_email:hover, #lineEdit_auth_email:hover, #lineEdit_auth_pass:hover, #notesName:hover, #lineEdit_remind_email:hover, #lineEdit_1:hover, #lineEdit_2:hover, #lineEdit_text_for_search:hover
	{
		border: 2px solid #FF9C0C;
		color: #1E90FF;
	}
	QLineEdit#lineEdit_reg_log:focus, #lineEdit_reg_pass:focus, #lineEdit_reg_pass_check:focus, #lineEdit_reg_email:focus, #lineEdit_auth_email:focus, #lineEdit_auth_pass:focus, #notesName:focus, #lineEdit_remind_email:focus, #lineEdit_1:focus, #lineEdit_2:focus, #lineEdit_text_for_search:focus
